<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserProfile"
    android:background="@color/backGround">

    <TextView
        android:id="@+id/etUserProfileFname"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:textSize="14dp"
        android:textColor="#000"
        android:paddingStart="15dp"
        android:text="FirstName"
        android:layout_marginHorizontal="@dimen/marginButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="20dp"
        android:paddingVertical="@dimen/paddingVertical"
        android:paddingHorizontal="@dimen/paddingHorizontal"
        />

    <TextView
        android:id="@+id/etUserProfileName"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:textSize="14dp"
        android:textColor="#000"
        android:paddingStart="15dp"
        android:textStyle="bold"
        android:textAllCaps="true"
        android:layout_marginHorizontal="@dimen/marginButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/etUserProfileFname"
        android:background="@drawable/rounded_textcolor"
        android:paddingVertical="@dimen/paddingVertical"
        android:paddingHorizontal="@dimen/paddingHorizontal"
        />

    <TextView
        android:id="@+id/etUserProfileLname"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:textSize="14dp"
        android:textColor="#000"
        android:paddingStart="15dp"
        android:text="LastName"
        android:layout_marginHorizontal="@dimen/marginButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/etUserProfileName"
        android:layout_marginTop="20dp"
        android:paddingVertical="@dimen/paddingVertical"
        android:paddingHorizontal="@dimen/paddingHorizontal"
        />

    <TextView
        android:id="@+id/etUserLastName"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:textSize="14dp"
        android:textColor="#000"
        android:paddingStart="15dp"
        android:textStyle="bold"
        android:textAllCaps="true"
        android:layout_marginHorizontal="@dimen/marginButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/etUserProfileLname"
        android:background="@drawable/rounded_textcolor"
        android:paddingVertical="@dimen/paddingVertical"
        android:paddingHorizontal="@dimen/paddingHorizontal"
        />

    <TextView
        android:id="@+id/etUserProfilePhone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:textSize="14dp"
        android:textColor="#000"
        android:paddingStart="15dp"
        android:text="Phone"
        android:layout_marginHorizontal="@dimen/marginButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/etUserLastName"
        android:layout_marginTop="20dp"
        android:paddingVertical="@dimen/paddingVertical"
        android:paddingHorizontal="@dimen/paddingHorizontal"
        />

    <TextView
        android:id="@+id/etUserPhoneNo"
        android:layout_width="0dp"
        android:padding="10dp"
        android:textSize="14dp"
        android:textColor="#000"
        android:paddingStart="15dp"
        android:textStyle="bold"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/marginButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/etUserProfilePhone"
        android:background="@drawable/rounded_textcolor"
        android:paddingVertical="@dimen/paddingVertical"
        android:paddingHorizontal="@dimen/paddingHorizontal"
        />


    <TextView
        android:id="@+id/etUserProfileEmail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:textSize="14dp"
        android:textColor="#000"
        android:paddingStart="15dp"
        android:text="EmailAddress"
        android:layout_marginHorizontal="@dimen/marginButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/etUserPhoneNo"
        android:layout_marginTop="20dp"
        android:paddingVertical="@dimen/paddingVertical"
        android:paddingHorizontal="@dimen/paddingHorizontal"
        />


    <TextView
        android:id="@+id/etUserEmail"
        android:layout_width="0dp"
        android:padding="10dp"
        android:textSize="14dp"
        android:textColor="#000"
        android:paddingStart="15dp"
        android:textStyle="bold"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/marginButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/etUserProfileEmail"
        android:background="@drawable/rounded_textcolor"
        android:paddingVertical="@dimen/paddingVertical"
        android:paddingHorizontal="@dimen/paddingHorizontal"
        />


</androidx.constraintlayout.widget.ConstraintLayout>